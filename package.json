{
  "name": "onemv-server",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "nodemon ./bin/www --exec babel-node --presets babel-preset-env",
    "eslint": "eslint **/*.js",
    "lint-staged": "lint-staged",
    "test-local": "nyc mocha --require babel-register tests/**/*.spec.js --timeout 10000 --exit",
    "test": "nyc --reporter=cobertura mocha --reporter mocha-junit-reporter --require babel-register tests/**/*.spec.js --timeout 10000 --exit "
  },
  "dependencies": {
    "amazon-cognito-identity-js": "^3.0.15",
    "app-root-path": "^2.2.1",
    "aws-sdk": "^2.585.0",
    "bcryptjs": "^2.4.3",
    "cookie-parser": "~1.4.3",
    "cors": "^2.8.5",
    "debug": "~2.6.9",
    "dotenv": "^8.0.0",
    "express": "~4.16.0",
    "express-validator": "^6.0.1",
    "googleapis": "^41.0.0",
    "http-errors": "~1.6.2",
    "jade": "~1.11.0",
    "jsonwebtoken": "^8.5.1",
    "jwk-to-pem": "^2.0.1",
    "moment": "^2.24.0",
    "mongoose": "^5.6.1",
    "morgan-body": "^2.4.7",
    "node-fetch": "^2.6.1",
    "querystring": "^0.2.0",
    "request": "^2.88.0",
    "winston": "^3.2.1",
    "winston-cloudwatch": "^2.0.7"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-eslint": "^10.0.1",
    "babel-loader": "^8.0.6",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-stage-3": "^6.24.1",
    "babel-register": "^6.26.0",
    "chai": "^4.2.0",
    "chai-as-promised": "^7.1.1",
    "chai-http": "^4.3.0",
    "eslint": "^5.16.0",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-plugin-import": "^2.17.3",
    "husky": "^2.4.0",
    "istanbul": "^0.4.5",
    "lint-staged": "^8.2.0",
    "mocha": "^6.1.4",
    "mocha-junit-reporter": "^1.23.1",
    "morgan": "^1.9.1",
    "node-mocks-http": "^1.7.6",
    "nodemon": "^1.19.1",
    "nyc": "^14.1.1",
    "sinon": "^7.3.2",
    "sinon-test": "^2.4.0"
  },
  "lint-staged": {
    "linters": {
      "*.js": [
        "eslint",
        "git add"
      ]
    },
    "ignore": [
      "**/dist/*.min.js"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  }
}
