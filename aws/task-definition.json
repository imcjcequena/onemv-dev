{
  "family": "frontend",
  "networkMode": "awsvpc",
  "containerDefinitions": [
    {
      "image": "708988062417.dkr.ecr.ap-southeast-2.amazonaws.com/ccequena:SAMPLES_89",
      "name": "frontend",
      "essential": true,
      "portMappings": [
        {
          "containerPort": 80,
          "hostPort": 80,
          "protocol": "tcp"
        },
        {
          "containerPort": 443,
          "hostPort": 443,
          "protocol": "tcp"
        }
      ]
    }
  ],
  "requiresCompatibilities": ["FARGATE"],
  "cpu": "256",
  "memory": "512"
}
