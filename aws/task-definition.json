{
  "family": "frontend",
  "networkMode": "awsvpc",
  "containerDefinitions": [
    {
      "image": "708988062417.dkr.ecr.ap-southeast-2.amazonaws.com/BUILD_TAG",
      "name": "frontend",
      "essential": true,
      "memoryReservation": 300,
      "portMappings": [
        {
          "containerPort": 80,
          "hostPort": 80,
          "protocol": "tcp"
        },
        {
          "containerPort": 443,
          "hostPort": 443,
          "protocol": "tcp"
        }
      ]
    }
  ],
  "requiresCompatibilities": ["FARGATE"],
  "cpu": "256",
  "memory": "512",

  "networkConfiguration": {
    "awsvpcConfiguration": {
      "assignPublicIp": "ENABLED",
      "securityGroups": ["sg-081e3ba176562e2b9"],
      "subnets": ["subnet-0837d4eadd365b9a7"]
    }
  }
}
