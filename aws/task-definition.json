{
  "family": "frontend",
  "containerDefinitions": [
    {
      "image": "708988062417.dkr.ecr.ap-southeast-2.amazonaws.com/BUILD_TAG",
      "name": "frontend",
      "essential": true,
      "cpu": 100,
      "memoryReservation": 300,
      "portMappings": [
        {
          "containerPort": 80,
          "hostPort": 80,
          "protocol": "tcp"
        },
        {
          "containerPort": 443,
          "hostPort": 443,
          "protocol": "tcp"
        }
      ]
    }
  ]
}
